{
    "name": "maestriam/filesystem",
    "description": "Manages files and folders for Maestriam packages.",
    "type": "package",
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.11"
    },
    "authors": [
        {
            "name": "Giuliano Sampaio",
            "email": "giuguitar@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Maestriam\\FileSystem\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Maestriam\\FileSystem\\": "src/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "Maestriam\\FileSystem\\Providers\\FileSystemProvider"
          ]
        }
    }
}
